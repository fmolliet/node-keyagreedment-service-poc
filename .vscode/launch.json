    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}\\src\\server.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Client",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}\\client\\Assymetric.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "GenerateKeys Client",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}\\client\\GenerateStaticKey.js"
        }
    ]
}